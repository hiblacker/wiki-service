{
    "printWidth": 120,
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "es5",
    "semi": false,
    "wrap_line_length": 120,
    "wrap_attributes": "auto",
    "proseWrap": "always",
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "useTabs": false,
    "eslintIntegration": true,
    "overrides": [
        {
            "files": ".prettierrc",
            "options": {
                "parser": "json"
            }
        }
    ],
    "endOfLine": "auto",
    "htmlWhitespaceSensitivity": "ignore"
}
